config BOARD_32F769IDISCOVERY
	bool "32F769IDISCOVERY"
	depends on CHIP_STM32F7
	select CHIP_STM32F769NI
	select BOARD_HAS_LEDS if CHIP_STM32_GPIOV2_GPIOI_ENABLE
	select BOARD_HAS_BUTTONS if CHIP_STM32_GPIOV2_GPIOI_ENABLE
	help
		32F769IDISCOVERY (STM32F769I-DISCO) board with STM32F769NI chip in BGA216 package

		Chip's supply voltage is 3.3 V.
		External 25 MHz clock can be used as HSE clock in "HSE Clock Bypass"
		mode.
