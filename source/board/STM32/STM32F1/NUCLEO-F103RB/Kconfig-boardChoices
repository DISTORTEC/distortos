config BOARD_NUCLEO_F103RB
	bool "NUCLEO-F103RB"
	depends on CHIP_STM32F1
	select CHIP_STM32F103RB
	select BOARD_HAS_BUTTONS if CHIP_STM32_GPIOV1_GPIOC_ENABLE
	help
		NUCLEO-F103RB board with STM32F103RB chip in LQFP64 package
