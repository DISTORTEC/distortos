		Enable USART8 low-level driver

config CHIP_STM32_USARTV2_HAS_CR1_M1_BIT
	bool
	default n

config CHIP_STM32_USARTV2_USART3_USART6_COMBINED_INTERRUPT
	bool
	default n

config CHIP_STM32_USARTV2_USART3_USART8_COMBINED_INTERRUPT
	bool
	default n

config CHIP_STM32_USARTV2_HAS_USART1
	bool
	default n

