if CHIP_STM32_USARTV2

config CHIP_STM32_USARTV2_USART1_ENABLE
	bool "USART1 low-level driver"
	default n
	depends on CHIP_STM32_USARTV2_HAS_USART1
	help
		Enable USART1 low-level driver

config CHIP_STM32_USARTV2_USART2_ENABLE
	bool "USART2 low-level driver"
	default n
	depends on CHIP_STM32_USARTV2_HAS_USART2
	help
		Enable USART2 low-level driver

