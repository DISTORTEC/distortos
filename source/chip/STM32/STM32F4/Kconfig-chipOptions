config CHIP_STM32F46
	bool
	default n
	select CHIP_STM32_GPIOV2_HAS_GPIOD
	select CHIP_STM32_GPIOV2_HAS_GPIOE
	select CHIP_STM32_GPIOV2_HAS_GPIOF
	select CHIP_STM32_GPIOV2_HAS_GPIOG
	select CHIP_STM32_GPIOV2_HAS_GPIOI
	select CHIP_STM32_GPIOV2_HAS_GPIOJ
	select CHIP_STM32_GPIOV2_HAS_GPIOK
	select CHIP_STM32_SPIV1_HAS_SPI2
	select CHIP_STM32_SPIV1_HAS_SPI3
	select CHIP_STM32_SPIV1_HAS_SPI4
	select CHIP_STM32_SPIV1_HAS_SPI5 if !CHIP_STM32F469V && !CHIP_STM32F469Z
	select CHIP_STM32_SPIV1_HAS_SPI6 if !CHIP_STM32F469V && !CHIP_STM32F469Z
	select CHIP_STM32_USARTV1_HAS_USART3
	select CHIP_STM32_USARTV1_HAS_UART4
