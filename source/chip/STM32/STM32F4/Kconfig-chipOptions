config CHIP_STM32F4_BKPSRAM_SIZE
	int
	default 4096 if CHIP_STM32F405 || CHIP_STM32F407 || CHIP_STM32F415 || CHIP_STM32F417 || CHIP_STM32F42 || \
		CHIP_STM32F43 || CHIP_STM32F44 || CHIP_STM32F46 || CHIP_STM32F47
	default 0

config CHIP_STM32F4_BKPSRAM_ADDRESS
	hex
	default 0x40024000
	depends on CHIP_STM32F4_BKPSRAM_SIZE != 0

config CHIP_STM32F4_CCM_SIZE
	int
	default 65536 if CHIP_STM32F405 || CHIP_STM32F407 || CHIP_STM32F415 || CHIP_STM32F417 || CHIP_STM32F42 || \
		CHIP_STM32F43 || CHIP_STM32F46 || CHIP_STM32F47
	default 0

config CHIP_STM32F4_CCM_ADDRESS
	hex
	default 0x10000000
	depends on CHIP_STM32F4_CCM_SIZE != 0

config CHIP_STM32F4_FLASH_SIZE
	int
	default 65536 if CHIP_STM32F410C8 || CHIP_STM32F410R8 || CHIP_STM32F410T8
	default 131072 if CHIP_STM32F401CB || CHIP_STM32F401RB || CHIP_STM32F401VB || CHIP_STM32F410CB || \
		CHIP_STM32F410RB || CHIP_STM32F410TB
	default 262144 if CHIP_STM32F401CC || CHIP_STM32F401RC || CHIP_STM32F401VC || CHIP_STM32F411CC || \
		CHIP_STM32F411RC || CHIP_STM32F411VC || CHIP_STM32F446MC || CHIP_STM32F446RC || CHIP_STM32F446VC || \
		CHIP_STM32F446ZC
	default 393216 if CHIP_STM32F401CD || CHIP_STM32F401RD || CHIP_STM32F401VD
	default 524288 if CHIP_STM32F401CE || CHIP_STM32F401RE || CHIP_STM32F401VE || CHIP_STM32F405OE || \
		CHIP_STM32F407IE || CHIP_STM32F407VE || CHIP_STM32F407ZE || CHIP_STM32F411CE || CHIP_STM32F411RE || \
		CHIP_STM32F411VE || CHIP_STM32F417IE || CHIP_STM32F417VE || CHIP_STM32F417ZE || CHIP_STM32F429BE || \
		CHIP_STM32F429IE || CHIP_STM32F429NE || CHIP_STM32F429VE || CHIP_STM32F429ZE || CHIP_STM32F446ME || \
		CHIP_STM32F446RE || CHIP_STM32F446VE || CHIP_STM32F446ZE || CHIP_STM32F469AE || CHIP_STM32F469BE || \
		CHIP_STM32F469IE || CHIP_STM32F469NE
	default 1048576 if CHIP_STM32F405OG || CHIP_STM32F405RG || CHIP_STM32F405VG || CHIP_STM32F405ZG || \
		CHIP_STM32F407IG || CHIP_STM32F407VG || CHIP_STM32F407ZG || CHIP_STM32F415 || CHIP_STM32F417IG || \
		CHIP_STM32F417VG || CHIP_STM32F417ZG || CHIP_STM32F427AG || CHIP_STM32F427IG || CHIP_STM32F427VG || \
		CHIP_STM32F427ZG || CHIP_STM32F429AG || CHIP_STM32F429BG || CHIP_STM32F429IG || CHIP_STM32F429NG || \
		CHIP_STM32F429VG || CHIP_STM32F429ZG || CHIP_STM32F437IG || CHIP_STM32F437VG || CHIP_STM32F437ZG || \
		CHIP_STM32F439BG || CHIP_STM32F439IG || CHIP_STM32F439NG || CHIP_STM32F439VG || CHIP_STM32F439ZG || \
		CHIP_STM32F469AG || CHIP_STM32F469BG || CHIP_STM32F469IG || CHIP_STM32F469NG || CHIP_STM32F479AG || \
		CHIP_STM32F479BG || CHIP_STM32F479IG || CHIP_STM32F479NG
	default 2097152 if CHIP_STM32F427AI || CHIP_STM32F427II || CHIP_STM32F427VI || CHIP_STM32F427ZI || \
		CHIP_STM32F429AI || CHIP_STM32F429BI || CHIP_STM32F429II || CHIP_STM32F429NI || CHIP_STM32F429VI || \
		CHIP_STM32F429ZI || CHIP_STM32F437AI || CHIP_STM32F437II || CHIP_STM32F437VI || CHIP_STM32F437ZI || \
		CHIP_STM32F439AI || CHIP_STM32F439BI || CHIP_STM32F439II || CHIP_STM32F439NI || CHIP_STM32F439VI || \
		CHIP_STM32F439ZI || CHIP_STM32F469AI || CHIP_STM32F469BI || CHIP_STM32F469II || CHIP_STM32F469NI || \
		CHIP_STM32F479AI || CHIP_STM32F479BI || CHIP_STM32F479II || CHIP_STM32F479NI
	default 0

config CHIP_STM32F4_FLASH_ADDRESS
	hex
	default 0x08000000
	depends on CHIP_STM32F4_FLASH_SIZE != 0

config CHIP_STM32F4_SRAM1_SIZE
	int
	default 32768 if CHIP_STM32F410
	default 65536 if CHIP_STM32F401CB || CHIP_STM32F401CC || CHIP_STM32F401RB || CHIP_STM32F401RC || \
		CHIP_STM32F401VB || CHIP_STM32F401VC
	default 98304 if CHIP_STM32F401CD || CHIP_STM32F401CE || CHIP_STM32F401RD || CHIP_STM32F401RE || \
		CHIP_STM32F401VD || CHIP_STM32F401VE
	default 114688 if CHIP_STM32F405 || CHIP_STM32F407 || CHIP_STM32F415 || CHIP_STM32F417 || CHIP_STM32F42 || \
		CHIP_STM32F43 || CHIP_STM32F44
	default 131072 if CHIP_STM32F411
	default 163840 if CHIP_STM32F46 || CHIP_STM32F47
	default 0

